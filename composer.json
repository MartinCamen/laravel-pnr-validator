{
    "name": "adaptivemedia/laravel-pnr-validator",
    "description": "Validate a swedish personnummer in Laravel",
    "keywords": [
        "laravel",
        "personnummer",
        "validate"
    ],
    "homepage": "https://adaptivemedia.se",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Eliasson",
            "email": "victor@adaptivemedia.se"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/translation": "^7",
        "illuminate/support": "^7",
        "illuminate/validation": "^7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.3",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Adaptivemedia\\PnrValidator\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adaptivemedia\\PnrValidator\\PnrValidatorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
